<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.14-alpha</Version>
    <Description>Extension methods for the dotnet/reactive library.</Description>
    <Authors>David Karnok</Authors>
    <PackageId>akarnokd.reactive_extensions</PackageId>
    <PackageLicenseUrl>https://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/akarnokd/reactive-extensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/akarnokd/reactive-extensions</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Copyright>Copyright 2018, David Karnok</Copyright>
    <PackageReleaseNotes>New operators:
+ ISingleSource
  - Amb
  - AmbAll
  - BlockingSubscribe
  - Cache
  - Concat
  - ConcatAll
  - ConcatEager
  - ConcatEagerAll
  - ConcatMap (on IObservable)
  - ConcatMapEager (on IObservable)
  - Defer
  - Delay
  - DelaySubscription
  - DoAfterSuccess
  - DoAfterTerminate
  - DoOnDispose
  - DoOnError
  - DoOnTerminate
  - DoOnSubscribe
  - DoOnSuccess
  - ElementAtOrDefault
  - ElementAtOrError
  - FirstOrDefault
  - FirstOrError
  - FlatMap (onto IEnumerable, onto ISingleSource, onto IObservable)
  - FromTask
  - LastOrDefault
  - LastOrError
  - Map
  - Merge
  - MergeAll
  - ObserveOn
  - OnErrorResumeNext
  - OnTerminateDetach
  - Repeat
  - RepeatWhen
  - Retry
  - RetryWhen
  - SingleOrDefault
  - SingleOrError
  - SubscribeOn
  - SubscribeSafe 
  - SwitchMap (on IObservable)
  - TakeUntil
  - Timeout
  - Timer
  - ToMaybe
  - ToObservable
  - ToSingle
  - ToTask
  - UnsubscribeOn
  - Using
  - Wait
  - Zip
</PackageReleaseNotes>
    <PackageTags>reactive, extensions</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\reactive-extensions.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\reactive-extensions.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Reactive" Version="4.0.0-preview.4.build.391" />
  </ItemGroup>

</Project>
