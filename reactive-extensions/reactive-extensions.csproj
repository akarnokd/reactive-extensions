<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Version>0.0.11-alpha</Version>
    <Description>Extension methods for the dotnet/reactive library.</Description>
    <Authors>David Karnok</Authors>
    <PackageId>akarnokd.reactive_extensions</PackageId>
    <PackageLicenseUrl>https://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/akarnokd/reactive-extensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/akarnokd/reactive-extensions</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <Copyright>Copyright 2018, David Karnok</Copyright>
    <PackageReleaseNotes>New operators:
+ IMaybeSource
  - BlockingSubscribe
  - Defer
  - Delay
  - DoAfterSuccess
  - DoAfterTerminate
  - DoFinally
  - DoOnCompleted
  - DoOnDispose
  - DoOnError
  - DoOnSubscribe
  - DoOnSuccess
  - DoOnTerminate
  - ElementAtIndex (on IObservable)
  - Filter
  - FirstElement (on IObservable)
  - FlatMap (to IMaybeSource, ISingleSource)
  - FromAction
  - FromFunc
  - FromTask
  - LastElement (on IObservable)
  - Map
  - OnErrorComplete
  - OnErrorResumeNext
  - Timeout
  - ToMaybe (on Task)
  - ToTask
  - SingleElement (on IObservable)
  - SubscribeSafe
  - Using
  - Wait
+ ISingleSource
  - FromFunc
  - Filter
  - FlatMap (to IMaybeSource)
</PackageReleaseNotes>
    <PackageTags>reactive, extensions</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.3\reactive-extensions.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard1.3\reactive-extensions.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Reactive.Interfaces" Version="4.0.0-preview.2.build.380" />
  </ItemGroup>

</Project>
